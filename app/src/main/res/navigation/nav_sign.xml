<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.creatives.vakansiyaaz.Login.SignInFragment"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_FirstFragment_to_emailPassFragment"
            app:destination="@id/emailPassFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.creatives.vakansiyaaz.Login.SignInUp.SignUpFragment"
        tools:layout="@layout/fragment_sign_up">
        <action
            android:id="@+id/action_SecondFragment_to_verficationFragment"
            app:destination="@id/verficationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/emailPassFragment"
        android:name="com.creatives.vakansiyaaz.Login.SignInUp.EmailPassFragment"
        android:label="fragment_email_pass"
        tools:layout="@layout/fragment_email_pass" >
        <action
            android:id="@+id/action_emailPassFragment_to_SecondFragment"
            app:destination="@id/SecondFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/verficationFragment"
        android:name="com.creatives.vakansiyaaz.Login.SignInUp.VerficationFragment"
        android:label="fragment_verfication"
        tools:layout="@layout/fragment_verfication" />
    <fragment
        android:id="@+id/numberSignFragment"
        android:name="com.creatives.vakansiyaaz.Login.Number.NumberSignFragment"
        android:label="fragment_number_sign"
        tools:layout="@layout/fragment_number_sign" >
        <action
            android:id="@+id/action_numberSignFragment_to_numberCodeFragment"
            app:destination="@id/numberCodeFragment" />
    </fragment>
    <fragment
        android:id="@+id/numberCodeFragment"
        android:name="com.creatives.vakansiyaaz.Login.Number.NumberCodeFragment"
        android:label="fragment_number_code"
        tools:layout="@layout/fragment_number_code" />
</navigation>